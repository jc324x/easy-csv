{
   "projectPath":"easy-csv-exports/apple-school-manager",
   "process":"exportSheets",
   "keepHeaders":"true",
   "trimToData":"true",
   "targets":[
      {
         "sheet":"locations"
      },
      {
         "sheet":"students",
         "range":"A:J"
      },
      {
         "sheet":"courses",
