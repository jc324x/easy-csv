      },
      {
         "sheet":"classes",
         "range":"A:E"
      },
      {
         "sheet":"rosters",
         "range":"A:C"
      },
      {
         "sheet":"staff",
         "range":"A:H"
      }
   ],
   "zipOutput":"true",
   "autoDownload":"true"
}
